    {
      "command": "SHOW_SPRITE",
      "slot": 3,
      "face": 0
    },
    {
      "command": "SHOW_TEXT",
      "text": "BOB: ..."
    },
    {
      "command": "SHOW_SPRITE",
      "slot": 1,
      "face": 3
    },
    {
      "command": "SHOW_TEXT",
      "text": "CINDY: I know, Lieutenant Bob, you know who I am."

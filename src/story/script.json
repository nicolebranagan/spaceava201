    {
      "command": "SHOW_SPRITE",
      "slot": 1,
      "face": 2
    },
    {
      "command": "SHOW_SPRITE",
      "slot": 0,
      "face": 2
    },
    {
      "command": "SHOW_TEXT",
      "text": "MAN: Schrodinger's cat is gone! They were weak and lost to the likes of you! We've got new leadership now!"
